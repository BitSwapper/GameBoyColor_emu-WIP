Thumbs.db
ehthumbs.db
Desktop.ini
$RECYCLE.BIN/
*.lnk

# Log files
*.log
logs/

# Temporary files
*.tmp
*.temp
temp/
tmp/

# Rider specific
.idea/
*.iml

# VS Code specific
.vscode/
